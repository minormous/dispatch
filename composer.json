{
    "name": "minormous/dispatch",
    "description": "An HTTP Interop compatible middleware dispatcher",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Minormous Contributors",
            "homepage": "https://github.com/minormous"
        }
    ],
    "require": {
        "php": ">=8.3",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "eloquent/phony-phpunit": "^3.0",
        "eloquent/liberator": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Minormous\\Dispatch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Minormous\\Dispatch\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
